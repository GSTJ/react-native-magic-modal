{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", ".svelte-kit/**"]
    },
    "doctor": {
      "cache": false
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["^topo", "^build"],
      "outputs": []
    },
    "format": {
      "outputs": ["node_modules/.cache/.formatcache"]
    },
    "format:fix": {
      "outputs": ["node_modules/.cache/.formatcache"]
    },
    "test": {
      "dependsOn": ["^topo"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"]
    },
    "copy:readme": {
      "cache": false
    },
    "release": {
      "dependsOn": ["^copy:readme"],
      "outputs": ["node_modules/.cache/release.json"]
    },
    "docs": {
      "dependsOn": ["^copy:readme"],
      "outputs": ["node_modules/.cache/docs.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
