{
  "name": "@magic/prettier-config",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules",
    "format:check": "prettier --check . --ignore-path ../../.gitignore",
    "format:write": "prettier --write . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "prettier": "catalog:"
  },
  "devDependencies": {
    "@magic/tsconfig": "workspace:*",
    "@types/node": "^22.14.0",
    "typescript": "catalog:"
  },
  "prettier": "@magic/prettier-config"
}
