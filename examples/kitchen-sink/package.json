{
  "name": "@magic/kitchen-sink",
  "description": "Example app for react-native-magic-modal",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start": "expo start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "doctor": "pnpm expo-doctor"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@magic/eslint-config/expo"
    ]
  },
  "dependencies": {
    "expo-doctor": "1.11.1",
    "expo": "^51.0.38",
    "expo-router": "^3.5.23",
    "expo-splash-screen": "0.27.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "2.16.2",
    "react-native-magic-modal": "workspace:*",
    "react-native-reanimated": "~3.16.0",
    "react-native-web": "~0.19.12"
  },
  "devDependencies": {
    "@babel/core": "~7.25.0",
    "@babel/runtime": "^7.24.5",
    "@magic/eslint-config": "workspace:*",
    "@magic/prettier-config": "workspace:*",
    "@magic/tsconfig": "workspace:*",
    "babel-preset-expo": "11.0.12"
  },
  "prettier": "@magic/prettier-config"
}
